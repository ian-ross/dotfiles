Config { font = "xft:Monaco:pixelsize=16:antialias=true:hinting=true",
         bgColor = "black",
         fgColor = "grey",
         position = TopW L 95,
         commands = [ Run Weather "LOXT" [ "-t", "<tempC>C <rh>%",
                                           "-L", "15", "-H", "27",
                                           "--normal", "green",
                                           "--high", "red",
                                           "--low", "lightblue"] 36000,
          Run Cpu [ "-t", "C:<total>", "-L", "3", "-H", "50",
                    "--normal", "green",
                    "--high", "red"] 10, 
          Run Memory [ "-t", "M:<usedratio>" ] 10, 
          Run Swap [ "-t", "S:<usedratio>" ] 10, 
          Run Date "%a %b %-d %H:%M" "date" 10, 
          Run StdinReader ], 
         sepChar = "%", alignSep = "}{", 
         template = "%StdinReader% }{ %cpu% %memory% %swap%  <fc=#ee9a00>%date%</fc>  %LOXT% " }
